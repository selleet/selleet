{
    "name": "selleet/selleet",
    "description": "Shopping software",
    "type": "project",
    "require": {
        "php": "^7.1",
        "ramsey/uuid": "^3.6"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.3",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Selleet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SelleetTest\\": "tests/features/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "bin-dir": "bin/",
        "sort-packages": true
    },
    "license": "MIT"
}
